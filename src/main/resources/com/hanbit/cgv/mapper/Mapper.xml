<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hanbit.cgv.mapper.Mapper">

	<select id="selectSome" resultType="List" parameterType="com.hanbit.cgv.command.Command">
		
		<choose>
			<when test="${table} = etc">
				select * from ${table}
			</when>
			<otherwise>
				select * from ${table}
			</otherwise>
		</choose>
	</select>
	
</mapper>